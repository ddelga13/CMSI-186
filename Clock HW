class ClockSolver {
	public static void main (String [] args){
		double slice = 0
		try{
			slice = Double.parseDouble (args [0]);
		} catch (ArrayIndexOutOfBoundsException e){
			System.out.println ("no argument given.");
		} catch (ArrayIndexOutOfBoundsException e) {
			System.out.println (e)
		}
		System.out.println (slice)
	}
}
